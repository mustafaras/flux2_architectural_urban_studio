  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "flux2_app",
      "vhost": "flux2",
      "configure": "^flux2\\..*",
      "write": "^flux2\\..*",
      "read": "^flux2\\..*"
    },
    {
      "user": "flux2_monitor",
      "vhost": "flux2",
      "configure": "",
      "write": "",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "exchanges": [
    {
      "name": "flux2.tasks",
      "vhost": "flux2",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "flux2.events",
      "vhost": "flux2",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "flux2.results",
      "vhost": "flux2",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "flux2.generation.requests",
      "vhost": "flux2",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 600000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "flux2.dlx"
      }
    },
    {
      "name": "flux2.generation.results",
      "vhost": "flux2",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 5000
      }
    },
    {
      "name": "flux2.model.requests",
      "vhost": "flux2",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-max-length": 1000
      }
    },
    {
      "name": "flux2.cache.invalidate",
      "vhost": "flux2",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "flux2.dlx.generation",
      "vhost": "flux2",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000
      }
    }
  ],
  "bindings": [
    {
      "source": "flux2.tasks",
      "destination": "flux2.generation.requests",
      "destination_type": "queue",
      "routing_key": "generation.*",
      "arguments": {}
    },
    {
      "source": "flux2.tasks",
      "destination": "flux2.model.requests",
      "destination_type": "queue",
      "routing_key": "model.*",
      "arguments": {}
    },
    {
      "source": "flux2.events",
      "destination": "flux2.cache.invalidate",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    }
  ]
